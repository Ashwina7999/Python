a=input()
sum1=0
for i in a:
    if(i.isalpha()==True):
        print("Invalid Input")
        break
    elif(i.isdigit()==True):
        z=int(i)
        sum1+=z
if(sum1!=0):        
    print("Sum of digits =",sum1)     

'''
TEST CASE 1:
A4d

OUTPUT:
Invalid Input

TEST CASE 2:
546546

OUTPUT:
Sum of digits = 30
'''
